<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane id="AnchorPane" prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.FXMLChatController">
  <children>
    <TextField fx:id="text" layoutX="14.0" layoutY="555.0" prefHeight="25.0" prefWidth="573.0" />
    <Button fx:id="send" layoutX="596.0" layoutY="555.0" mnemonicParsing="false" onAction="#handleSendAction" text="Odeslat" />
    <Label fx:id="nick" layoutX="687.0" layoutY="14.0" prefHeight="16.0" prefWidth="100.0" text="" />
    <Button fx:id="logout" layoutX="687.0" layoutY="53.0" mnemonicParsing="false" onAction="#handleLogAction" text="Odhlásit se" />
    <Button fx:id="insert" layoutX="672.0" layoutY="555.0" mnemonicParsing="false" onAction="#handleInsertFileAction" text="Vložit soubor" />
    <ScrollPane fx:id="scroll" fitToWidth="true" layoutX="14.0" layoutY="14.0" prefHeight="525.0" prefWidth="653.0">
         <content>
            <TextFlow fx:id="chat" prefHeight="522.0" prefWidth="650.0">
               <opaqueInsets>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </opaqueInsets>
            </TextFlow>
         </content>
      </ScrollPane>
     
      
  </children>
</AnchorPane>
